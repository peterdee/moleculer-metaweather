{
  "name": "moleculer-metaweather",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Peter Dyumin",
    "url": "https://github.com/peterdee"
  },
  "description": "A Moleculer-based microservice that pulls information from the MetaWeather",
  "homepage": "https://github.com/peterdee/moleculer-metaweather",
  "scripts": {
    "dev": "NODE_ENV=development moleculer-runner --env --repl --hot services/**/*.service.js",
    "start": "NODE_ENV=production moleculer-runner --env",
    "cli": "moleculer connect",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "engines": {
    "node": "14.4.0",
    "npm": "6.14.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterdee/moleculer-metaweather"
  },
  "keywords": [
    "MetaWeather",
    "Microservice",
    "Moleculer",
    "Node"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moleculer": "^0.14.0",
    "moleculer-web": "^0.9.0",
    "nats": "^1.4.8"
  },
  "devDependencies": {
    "moleculer-repl": "^0.6.2"
  }
}
