  },
  "keywords": [
    "microservice",
    "moleculer"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "moleculer": "^0.14.0",
    "moleculer-web": "^0.9.0"
  },
  "devDependencies": {
    "moleculer-repl": "^0.6.2"
  }
}
