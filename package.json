{
  "name": "moleculer-metaweather",
  "author": "",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --env --repl --hot services/**/*.service.js",
    "start": "moleculer-runner",
